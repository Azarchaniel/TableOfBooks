Attribute VB_Name = "prvyZaznam"
Sub tlacitko_prvy_zaznam()
'
' go to the first row directly
'

If ActiveSheet.Name = "Knihy_L'uboš" Or ActiveSheet.Name = "Knihy_Žanetka" Then
    ActiveSheet.Range("$N$4").Select
End If
If ActiveSheet.Name = "LP" Then
    ActiveSheet.Range("$B$4").Select
End If
End Sub
